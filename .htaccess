<IfModule mod_rewrite.c>

Options +FollowSymLinks
RewriteEngine on

#RewriteCond %{HTTP_HOST} ^www\.(.*) [NC]
#RewriteRule ^(.*)$ http://%1/$1 [R=301,L]
RewriteRule (heroes)/(.*) - [L]
RewriteRule (html)/(.*) - [L]
RewriteRule (museimvd)/(.*) - [L]

RewriteRule    ^$ app/webroot/    [L]
RewriteRule    (.*) app/webroot/$1 [L]
</IfModule>